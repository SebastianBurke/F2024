@model scbH60Store.Models.ProductCategory

<h2>Create Product Category</h2>

<form asp-action="Create" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="ProdCat" class="control-label"></label>
        <input asp-for="ProdCat" class="form-control" />
        <span asp-validation-for="ProdCat" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="ImageFile">Category Image</label>
        <input type="file" name="ImageFile">
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>
